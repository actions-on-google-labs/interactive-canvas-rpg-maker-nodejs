{
    "extends": ["eslint:recommended", "google"],
    "plugins": [
        "actions-on-google"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-undef": "off",
        "valid-jsdoc": "off",
        "spaced-comment": "off"
    }
}